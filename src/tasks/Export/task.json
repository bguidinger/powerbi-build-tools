{
  "id": "2923BF11-733A-4143-9654-0D0A155507E5",
  "name": "PowerBIExport",
  "friendlyName": "Power BI Export",
  "description": "Refresh a dataset or dataflow.",
  "author": "bguidinger",
  "category": "Azure Pipelines",
  "visibility": [
    "Build",
    "Release"
  ],
  "demands": [
    "powershell"
  ],
  "version": {
    "Major": "1",
    "Minor": "0",
    "Patch": "0"
  },
  "instanceNameFormat": "Power BI Export: $(Name)",
  "inputs": [
    {
      "name": "Connection",
      "aliases": [
        "connection"
      ],
      "type": "connectedService:Power-BI",
      "label": "Power BI connection",
      "required": true,
      "helpMarkDown": "The service connection to Power BI."
    },
    {
      "name": "Workspace",
      "aliases": [
        "workspace"
      ],
      "type": "string",
      "label": "Workspace",
      "required": false,
      "helpMarkDown": "The ID or name of the workspace. If not specified, your personal workspace will be used."
    },
    {
      "name": "Name",
      "aliases": [
        "name"
      ],
      "type": "string",
      "label": "Name",
      "required": true,
      "helpMarkDown": "The ID or name of the dataset or dataflow."
    },
    {
      "name": "FileFormat",
      "aliases": [
        "fileFormat"
      ],
      "type": "pickList",
      "label": "File Format",
      "required": true,
      "options": {
        "PBIX": "PBIX",
        "PDF": "PDF",
        "CSV": "CSV",
        "XML": "XML",
        "DOCX": "DOCX",
        "PPTX": "PPTX",
        "XLSX": "XLSX",
        "IMAGE": "IMAGE",
        "PNG": "PNG",
        "MHTML": "MHTML"
      },
      "defaultValue": "PDF"
    }
  ],
  "execution": {
    "PowerShell3": {
      "target": "Run.ps1"
    }
  }
}