{
  "id": "59D514DF-097E-4728-9E74-229149AB27AD",
  "name": "PowerBIDataRefresh",
  "friendlyName": "Power BI Data Refresh",
  "description": "Refresh a dataset or dataflow.",
  "author": "bguidinger",
  "category": "Azure Pipelines",
  "visibility": [
    "Build",
    "Release"
  ],
  "demands": [
    "powershell"
  ],
  "version": {
    "Major": "1",
    "Minor": "0",
    "Patch": "0"
  },
  "instanceNameFormat": "Power BI Data Refresh: $(Dataset)",
  "inputs": [
    {
      "name": "Connection",
      "aliases": [
        "connection"
      ],
      "type": "connectedService:Power-BI",
      "label": "Power BI connection",
      "required": true,
      "helpMarkDown": "The service connection to Power BI."
    },
    {
      "name": "Workspace",
      "aliases": [
        "workspace"
      ],
      "type": "string",
      "label": "Workspace",
      "required": false,
      "helpMarkDown": "The ID or name of the workspace. If not specified, your personal workspace will be used."
    },
    {
      "name": "Type",
      "aliases": [
        "type"
      ],
      "type": "pickList",
      "label": "Type",
      "required": true,
      "options": {
        "Dataset": "Dataset",
        "Dataflow": "Dataflow"
      },
      "defaultValue": "Dataset"
    },
    {
      "name": "Name",
      "aliases": [
        "name"
      ],
      "type": "string",
      "label": "Name",
      "required": true,
      "helpMarkDown": "The ID or name of the dataset or dataflow."
    },
    {
      "name": "NotifyOption",
      "aliases": [
        "notifyOption"
      ],
      "type": "pickList",
      "label": "Notify Option",
      "required": true,
      "options": {
        "NoNotification": "No Notification",
        "MailOnFailure": "Mail on Failure",
        "MailOnCompletion": "Mail on Completion"
      },
      "defaultValue": "NoNotification"
    }
  ],
  "execution": {
    "PowerShell3": {
      "target": "Run.ps1"
    }
  }
}