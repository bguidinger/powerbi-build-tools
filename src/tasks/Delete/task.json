{
  "id": "090A85A7-EBFC-4686-8F6F-5095815DBAFD",
  "name": "PowerBIDelete",
  "friendlyName": "Power BI Delete",
  "description": "Deletes a workspace, dataset, dataflow, or report.",
  "author": "bguidinger",
  "category": "Azure Pipelines",
  "visibility": [
    "Build",
    "Release"
  ],
  "demands": [
    "powershell"
  ],
  "version": {
    "Major": "1",
    "Minor": "0",
    "Patch": "0"
  },
  "instanceNameFormat": "Power BI Delete: $(Name)",
  "inputs": [
    {
      "name": "Connection",
      "aliases": [
        "connection"
      ],
      "type": "connectedService:Power-BI",
      "label": "Power BI connection",
      "required": true,
      "helpMarkDown": "The service connection to Power BI."
    },
    {
      "name": "Workspace",
      "aliases": [
        "workspace"
      ],
      "type": "string",
      "label": "Workspace",
      "required": false,
      "helpMarkDown": "The ID or name of the workspace. If not specified, your personal workspace will be used.",
      "visibleRule": "Type != Workspace"
    },
    {
      "name": "Type",
      "aliases": [
        "type"
      ],
      "type": "pickList",
      "label": "Type",
      "required": true,
      "options": {
        "Workspace": "Workspace",
        "Dataset": "Dataset",
        "Dataflow": "Dataflow",
        "Report": "Report"
      },
      "defaultValue": "Workspace"
    },
    {
      "name": "Name",
      "aliases": [
        "name"
      ],
      "type": "string",
      "label": "Name",
      "required": true,
      "helpMarkDown": "The ID or name of the object."
    }
  ],
  "execution": {
    "PowerShell3": {
      "target": "Run.ps1"
    }
  }
}